echo
echo ORSZAG-TANG CASE 1...
./ORS-TANG_TEST.out inputs/ORS-TANGinput128 outputs/ORS-TANGoutput128 \
plots/ORS-TANGplot128 -quiet
echo
echo ORSZAG-TANG CASE 2...
./ORS-TANG_TEST.out inputs/ORS-TANGinput256 outputs/ORS-TANGoutput256 \
plots/ORS-TANGplot256 -quiet
gnuplot plots/ORS-TANGplotA.plt


